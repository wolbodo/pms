{
  "name": "pms-frontend",
  "version": "1.0.0",
  "description": "The react frontend for the membership management system",
  "main": "index.jsx",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --optimize-dedupe --optimize-minimize",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "author": "Dexter",
  "license": "ISC",
  "dest": {
    "app": "app.js",
    "dist": "build",
    "vendor": "build/vendor"
  },
  "paths": {
    "jsx": "./app/**/*.jsx",
    "html": "./app/*.html",
    "less": "./app/*.less",
    "json": "./app/*.json",
    "img": "./app/img/*"
  },
  "dependencies": {
    "canonical-reducer-composition-validator": "^1.3.1",
    "classnames": "^2.2.3",
    "fetch": "^1.0.0",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^3.10.0",
    "material-design-icons": "^2.1.3",
    "material-design-lite": "^1.0.6",
    "material-ui": "^0.14.2",
    "react": "^0.14.6",
    "react-addons-update": "^0.14.6",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dnd-touch-backend": "^0.2.4",
    "react-dom": "^0.14.6",
    "react-mdl": "^1.2.0",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-router-redux": "^2.1.0",
    "react-tap-event-plugin": "^0.2.1",
    "react-ui-datepicker": "^0.1.0",
    "redux": "^3.0.5",
    "redux-diff-logger": "0.0.9",
    "redux-immutable": "^1.3.10",
    "redux-localstorage": "*",
    "redux-localstorage-immutable": "^1.0.4",
    "redux-perf-middleware": "^1.0.1",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-1": "^6.3.13",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "rimraf": "^2.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
